{
  "name": "discord-digital-twin",
  "version": "0.0.1",
  "description": "Local Discord API twin for testing discord.js bots without hitting real Discord servers",
  "type": "module",
  "license": "MIT",
  "author": "remorses <beats.by.morse@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/remorses/kimaki",
    "directory": "discord-digital-twin"
  },
  "homepage": "https://github.com/remorses/kimaki/tree/main/discord-digital-twin",
  "bugs": "https://github.com/remorses/kimaki/issues",
  "keywords": [
    "discord",
    "testing",
    "mock",
    "discord.js",
    "e2e",
    "integration-test"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./src": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "generate": "prisma generate",
    "build": "tsc",
    "prepublishOnly": "pnpm build",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@prisma/adapter-libsql": "7.3.0",
    "@prisma/client": "7.3.0",
    "discord-api-types": "^0.38.16",
    "spiceflow": "^1.17.12",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.0",
    "discord.js": "^14.25.1",
    "prisma": "7.3.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
